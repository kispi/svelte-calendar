{
  "name": "svelte-calendar",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "start": "node -r dotenv/config build"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.45.6",
    "svelte-check": "^4.3.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@auth/drizzle-adapter": "^1.11.1",
    "@auth/sveltekit": "^1.11.1",
    "@google/generative-ai": "^0.24.1",
    "@tanstack/svelte-query": "^6.0.14",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "ics": "^3.8.1",
    "korean-lunar-calendar": "^0.3.6",
    "mysql2": "^3.16.0",
    "node-ical": "^0.23.1"
  }
}